      <div class="table_component" role="region" tabindex="0">
        <table>
            <caption>Top <%=result.length%> Crypto</caption>
            <thead>
                <tr>
                    <th>Sl No.</th>
                    <th>Name</th>
                    <th>Price</th>
                    <th>Volume(24h)</th>
                    <th>1h %</th>
                    <th>24h %</th>
                    <th>7d %</th>
                    <th>Market Cap</th>
                    <th>Circulating Supply</th>
                    <th>Last 7 Days</th>
                    <th>Ratio</th>
                    <th>Adjusted Ratio</th>
                    <th>Allocated</th>
                    <th>Amount</th>
                </tr>
            </thead>
            <tbody>
              <% for(var i=0;i<result.length;i++){ %>
              <tr>
                <td>
                  <%= i+1 %>
                </td>
                <td>
                  <%=result[i].name%>
                </td>
                <td>
                  <%=(result[i].quote.USD.price).toFixed(2)%>
                </td>
                <td>
                  <%=(result[i].quote.USD.volume_24h).toFixed(2) %>
                </td>
                <td>
                  <%=(result[i].quote.USD.percent_change_1h).toFixed(2) %>
                </td>
                <td>
                  <%=(result[i].quote.USD.percent_change_24h).toFixed(2) %>
                </td>
                <td>
                  <%=(result[i].quote.USD.percent_change_7d).toFixed(2) %>
                </td>
                <td>
                  <%=(result[i].quote.USD.market_cap).toFixed(2) %>
                </td>
                <td>
                  <%=(result[i].circulating_supply).toFixed(2) %>
                </td>
                <td>
                  <%=(result[i].total_supply).toFixed(2) %>
                </td>
                <td>
                  <%=(((result[i].quote.USD.volume_24h)*100)/(totalVolume)).toFixed(2) %>
                </td>
                <td><% if(i < nratio){ %>  
                      <%= ratio %>  
                    <% }else{ %>
                      <%= ((((result[i].quote.USD.volume_24h)*100)/(totalVolume))/restRatio)*(1-((ratio*nratio)/100))*100 %> 
                    <% } %>
                </td>
                <td>
                  <% if(i < nratio){ %>  
                    <%= (ratio/100)*volume %>  
                  <% }else{ %>
                    <%=(((((result[i].quote.USD.volume_24h)*100)/(totalVolume))/restRatio)*(1-((ratio*nratio)/100)))*volume %> 
                  <% } %>
                </td>
                <td>
                  <% if(i < nratio){ %>  
                    <%= ((ratio/100)*volume)/(result[i].quote.USD.volume_24h) %>  
                  <% }else{ %>
                    <%= ((((((result[i].quote.USD.volume_24h)*100)/(totalVolume))/restRatio)*(1-((ratio*nratio)/100)))*volume)/(result[i].quote.USD.volume_24h) %> 
                  <% } %>
                </td>
            </tr>
            <% } %>
            </tbody>
        </table>
